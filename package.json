{
  "name": "minesweeper-react",
  "description": "An XP Minesweeper clone using react (for fun).",
  "author": "Travis Wiertzema",
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:js": "webpack",
    "build:types": "tsc --emitDeclarationOnly",
    "doc:readme": "documentation readme src/lib/utils.js -s API -c documentation.yml",
    "doc:serve": "documentation serve src/lib/utils.js -f md -c documentation.yml",
    "start": "webpack-dev-server --open",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "jest"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 Edge versions"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@teamsupercell/typings-for-css-modules-loader": "^2.1.0",
    "@types/jest": "^25.1.2",
    "@types/seedrandom": "^2.4.28",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "documentation": "^12.1.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-promise": "^4.2.1",
    "file-loader": "^5.0.2",
    "jest": "^25.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "1.19.1",
    "seedrandom": "^3.0.5",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
